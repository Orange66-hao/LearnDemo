OrderStatus.ORDER_ROG = 6; // 已收货
OrderStatus.ORDER_COMPLETE = 7; // 已完成
OrderStatus.ORDER_CANCELLATION = 8; // 作废
OrderStatus.ORDER_NOT_CONFIRM = 9; // 已生效

